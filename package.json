{
  "name": "svelte-select",
  "version": "5.8.3",
  "type": "module",
  "license": "ISC",
  "description": "A <Select> component for Svelte apps",
  "packageManager": "pnpm@9.12.2",
  "repository": "https://rob-balfre@github.com/rob-balfre/svelte-select.git",
  "author": "Robert Balfr√© <rob.balfre@gmail.com> (https://github.com/rob-balfre)",
  "keywords": [
    "svelte"
  ],
  "files": ["dist"],
  "exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
  },
  "svelte": "./dist/index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "test": "npm run test:dev & npm run test:browser",
    "test:dev": "rollup -cw",
    "test:browser": "sirv test/public -p 3000 --dev --open",
    "gen:docs": "node docs/generate_theming_variables_md.cjs",
    "preprepare": "node src/remove-styles.cjs",
    "prepare": "svelte-package",
    "postprepare": "node src/post-prepare.cjs && npm run gen:docs"
  },
  "dependencies": {
    "svelte-floating-ui": "1.5.9"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.4",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/adapter-static": "2.0.1",
    "@sveltejs/kit": "^1.30.4",
    "@sveltejs/package": "^2.3.5",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "find-in-files": "^0.5.0",
    "fuse.js": "^6.6.2",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "rollup": "^3.29.5",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^7.2.2",
    "sirv-cli": "^2.0.2",
    "svelte": "^4.2.19",
    "svelte-highlight": "^7.7.0",
    "svelte-tiny-virtual-list": "^2.1.2",
    "tape-modern": "^1.1.2",
    "typescript": "^5.6.3",
    "vite": "^4.5.5"
  }
}
